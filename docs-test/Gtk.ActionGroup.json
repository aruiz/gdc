{
    "constructors": [
        {
            "doc": "<p>Creates a new <span data-xref=\"Gtk.ActionGroup\"/> object. The name of the action group\nis used when associating [keybindings][Action-Accel]\nwith the actions.</p>",
            "kind": "constructor",
            "name": "new",
            "parameters": [
                {
                    "allow_none": false,
                    "argname": "name",
                    "direction": "in",
                    "type": "String"
                }
