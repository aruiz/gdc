    "name": "CellRendererAccel",
    "properties": [
        {
            "doc": "<p>The keyval of the accelerator.</p>",
            "kind": "property",
            "name": "accel-key",
            "type": "Number(guint)"
        },
        {
            "doc": "<p>Determines if the edited accelerators are GTK+ accelerators. If\nthey are, consumed modifiers are suppressed, only accelerators\naccepted by GTK+ are allowed, and the accelerators are rendered\nin the same way as they are in menus.</p>",
            "kind": "property",
            "name": "accel-mode",
            "type": "Gtk.CellRendererAccelMode"
        },
        {
            "doc": "<p>The modifier mask of the accelerator.</p>",
            "kind": "property",
            "name": "accel-mods",
            "type": "Gdk.ModifierType"
        },
        {
            "doc": "<p>The hardware keycode of the accelerator. Note that the hardware keycode is\nonly relevant if the key does not have a keyval. Normally, the keyboard\nconfiguration should assign keyvals to all keys.</p>",
            "kind": "property",
            "name": "keycode",
            "type": "Number(guint)"
        }
    ],
